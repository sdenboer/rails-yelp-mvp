<!DOCTYPE html>
<html>
  <head>
    <title>RailsYelpMvp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <%= stylesheet_link_tag     'application', 'https://fonts.googleapis.com/css?family=Raleway:500' %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="navbar"><%= link_to 'YELP', restaurants_path %><%= link_to 'New Restaurant', new_restaurant_path %></div>
    <%= yield %>
    <div class="footer">Sven den Boer</div>
  </body>
</html>
